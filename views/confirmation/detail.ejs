<div class="row">
    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
        <div class="profile-info-inner">
            <form action="/photo" method="POST" enctype="multipart/form-data">
                <div class="profile-img">
                    <div class="file-button">
                        <img src="/images/uploads/<%= data[0].image%>" alt="<%= data[0].image%>" />
                    </div>
                </div>
            </form>
        </div>
    </div>
    <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
        <div class="product-status-wrap">
            <h4>Detail Pembayaran Merchant</h4>
            <div id="myTabContent" class="tab-content custom-product-edit st-prf-pro">
                <div class="product-tab-list tab-pane fade active in" id="description">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="review-content-section">
                                <div class="row">
                                    <form action="#">
                                        <div class="col-lg-12">
                                            <div class="form-group-inner">
                                                <div class="row">
                                                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                                        <div class="content-profile">
                                                            <label class="login2">Nama Toko</label>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
                                                        <label class="login2">:</label>
                                                        <label class="login2">
                                                            <%= data[0].name%></label>
                                                        <!-- <input type="text" class="form-control" name="name"/> -->
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-12">
                                            <div class="form-group-inner">
                                                <div class="row">
                                                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                                        <label class="login2">Kode Transaksi</label>
                                                    </div>
                                                    <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
                                                        <label class="login2">:</label>
                                                        <label class="login2">
                                                            <%= data[0].code%></label>
                                                        <!-- <input type="password" class="form-control" placeholder="password" /> -->
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-12">
                                            <div class="form-group-inner">
                                                <div class="row">
                                                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                                        <label class="login2">Total</label>
                                                    </div>
                                                    <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
                                                        <label class="login2">:</label>
                                                        <label class="login2">
                                                            <%= data[0].total%></label>
                                                        <!-- <input type="password" class="form-control" placeholder="password" /> -->
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-12">
                                            <div class="form-group-inner">
                                                <div class="row">
                                                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                                        <label class="login2">Status</label>
                                                    </div>
                                                    <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
                                                        <label class="login2">:</label>
                                                        <label class="login2">
                                                            <%= data[0].status%></label>
                                                        <!-- <input type="password" class="form-control" placeholder="password" /> -->
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>