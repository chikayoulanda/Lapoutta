<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <div class="sparkline12-list">
        <div class="sparkline12-hd">
            <div class="main-sparkline12-hd">
                <h1>Konfirmasi Billing Merchant</h1>
            </div>
        </div>
        <div class="sparkline12-graph">
            <div class="basic-login-form-ad">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="all-form-element-inner">
                            <form action="/confirm/billing" method="POST" enctype="multipart/form-data">
                                
                                <div class="form-group-inner">
                                    <div class="row">
                                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
                                            <label class="login2 pull-right pull-right-pro">Kode Order</label>
                                        </div>
                                        <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12">
                                            <div>
                                                <% _.each(_billing, function(bill) { %>
                                                    <% if(bill.status=="Belum dibayar"){%>
                                                    <label>
                                                        <input type="checkbox" name="<%=bill.code%>" value="<%=bill.id%>"><%=bill.code%> <br>
                                                    </label>
                                                    <%}%>
                                                <%})%>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group-inner">
                                    <div class="row">
                                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
                                            <label class="login2 pull-right pull-right-pro">Bukti pembayaran</label>
                                        </div>
                                        <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12">
                                            <input type="file" name="image" id="image">
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group-inner">
                                    <div class="row">
                                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
                                        </div>
                                        <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12">
                                            <button class="btn btn-sm btn-primary login-submit-cs"
                                                type="submit">Konfirmasi</button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>