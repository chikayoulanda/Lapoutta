<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
  <div class="sparkline12-list">
    <div class="sparkline12-hd">
      <div class="main-sparkline12-hd">
        <h1>Tambah Referensi Bank</h1>
      </div>
    </div>
    <div class="sparkline12-graph">
      <div class="basic-login-form-ad">
        <div class="row">
          <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="all-form-element-inner">
              <form action="/bank/add" method="POST">
                <div class="form-group-inner">
                  <div class="row">
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
                      <label class="login2">Name</label>
                    </div>
                    <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12">
                      <input type="text" class="form-control" placeholder="Chika Youlanda" id="name" name="name" value="" />
                    </div>
                  </div>
                </div>
                <div class="form-group-inner">
                  <div class="row">
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
                      <label class="login2">No. Rekening</label>
                    </div>
                    <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12">
                      <input type="text" class="form-control" placeholder="456789" id="no_rek" name="no_rek" value="" />
                    </div>
                  </div>
                </div>
                <div class="form-group-inner"></div>
                <div class="row">
                  <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
                    <label class="login2">Nama Bank</label>
                  </div>
                  <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12">
                    <input type="text" class="form-control" placeholder="mandiri" id="bank" name="bank" value="" />
                  </div>
                </div>
            </div>
            <div class="form-group-inner"></div>
              <div class="row">
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12"></div>
                <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12">
                  <div class="login-horizental">
                    <button class="btn btn-sm btn-primary login-submit-cs" type="submit">Save</button>
                  </div>
                </div>
              </div>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>