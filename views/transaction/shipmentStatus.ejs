<div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
    <div class="login-horizental">
        <form action="/new/shipment/status" method="GET">
            <button class="btn btn-sm btn-primary login-submit-cs" type="submit">Tambah Status Pengiriman</button>
        </form>
    </div>
</div>

<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <div class="sparkline13-list">
        <div class="sparkline13-hd">
            <div class="main-sparkline13-hd">
                <h1><span class="table-project-n">Status Pengiriman Barang</span></h1>
            </div>
        </div>
        <div class="sparkline13-hd">
            <table id="shipment" class="table table-bordered table-striped" width="100%">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Nama Status</th>
                        <th>Aksi</th>
                    </tr>
                </thead>
                <tbody>
                    <%
            for(var _index=0; _index < shipment.length; _index++) {
                var _shipment = shipment[_index];
            %>

                        <tr>
                            <td>
                                <%=_shipment.id%>
                            </td>
                            <td>
                                <%=_shipment.name_shipment_status%>
                            </td>
                            <td>
                                <div style="float:left">
                                    <!-- <button>Edit</button> -->
                                    <a class="btn btn-warning" href="/edit/shipment/<%=_shipment.id%>">Edit</a> &nbsp;
                                    <a class="btn btn-danger" href="/delete/shipment/status/<%=_shipment.id%>">Delete</a>
                                </div>
                            </td>
                        </tr>
                        <%}%>

                </tbody>
            </table>
        </div>
    </div>
</div>
